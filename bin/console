require('dotenv').config()
const dbConfig = require('../knexfile')[process.env.NODE_ENV]
const repl = require('repl');

console.log(`Starting console in ${process.env.NODE_ENV} env`)
const consoleProcess = repl.start()

require('dotenv').config()

consoleProcess.context.knex = require('knex')(dbConfig)
consoleProcess.context.userService = require('../components/user/userService')
consoleProcess.context.User = require('../components/user/user')
